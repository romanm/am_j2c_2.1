{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"id": {
			"type": "string"
		},
		"type": {
			"type": "string"
		},
		"status": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"number": {
			"type": "string",
			"description": "Unique human readable episode of care number. Must be generated by the client before sending request"
		},
		"managing_organization": {
			"type": "object",
			"properties": {
				"identifier": {
					"type": "object",
					"properties": {
						"type": {
							"type": "object",
							"properties": {
								"coding": {
									"type": "array",
									"items": {
										"type": "object",
										"properties": {
											"system": {
												"type": "string"
											},
											"code": {
												"type": "string",
												"description": "Symbol in syntax defined by the system"
											}
										},
										"required": [
											"code"
										]
									}
								},
								"text": {
									"type": "string"
								}
							},
							"description": "Description of identifier"
						},
						"value": {
							"type": "string",
							"description": "The value that is unique"
						}
					},
					"required": [
						"type",
						"value"
					]
				}
			}
		},
		"period": {
			"type": "object",
			"properties": {
				"start": {
					"type": "string",
					"description": "C? Starting time with inclusive boundary"
				},
				"end": {
					"type": "string",
					"description": "C? End time with inclusive boundary, if not ongoing"
				}
			},
			"required": [
				"start"
			]
		},
		"care_manager": {
			"type": "object",
			"properties": {
				"identifier": {
					"type": "object",
					"properties": {
						"type": {
							"type": "object",
							"properties": {
								"coding": {
									"type": "array",
									"items": {
										"type": "object",
										"properties": {
											"system": {
												"type": "string"
											},
											"code": {
												"type": "string",
												"description": "Symbol in syntax defined by the system"
											}
										},
										"required": [
											"code"
										]
									}
								},
								"text": {
									"type": "string"
								}
							},
							"description": "Description of identifier"
						},
						"value": {
							"type": "string",
							"description": "The value that is unique"
						}
					},
					"required": [
						"type",
						"value"
					]
				}
			}
		}
	},
	"required": [
		"id",
		"type",
		"name",
		"number",
		"managing_organization",
		"period",
		"care_manager"
	]
}